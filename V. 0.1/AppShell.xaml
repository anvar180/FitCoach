<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="findgymbro.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:findgymbro"
    Shell.FlyoutBehavior="Disabled"
    Title="findgymbro">

    <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="ChatPage"
        ContentTemplate="{DataTemplate local:ChatPage}"
        Route="ChatPage" />

    <ShellContent
        Title="ChatPageList"
        ContentTemplate="{DataTemplate local:ChatPageList}"
        Route="ChatPageList" />

</Shell>
